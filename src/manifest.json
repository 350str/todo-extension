{
    "name": "Todo app",
    "version": "1.0",
    "description": "Todo list for testing",
    "manifest_version": 2,
    "permissions": [ "storage" ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "images/icon16.png"
    },
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
}